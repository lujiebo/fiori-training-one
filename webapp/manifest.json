{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.shunyu.lujb.fiori-training-one",
		"type": "application",
		"i18n": "i18n/i18n_en.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{TIT_APP_TITLE}}",
		"description": "{{TIT_APP_DESCRIPTION}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.shunyu.lujb.fiori-training-one.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.shunyu.lujb.fiori-training-one.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.shunyu.lujb.fiori-training-one.view",
				"controlAggregation": "pages",
				"controlId": "app"
			},
			"routes": [{
				"pattern": "",
				"name": "startTrip",
				"target": ["startTrip"]
			},{
				"pattern": "createTicket/{ticketfrom}",
				"name": "createTicket",
				"target": ["createTicket"]
			},{
				"pattern": "endTrip",
				"name": "endTrip",
				"target": ["endTrip"]
			}],
			"targets": {
				"startTrip": {
					"viewName": "StartTrip",
					"viewId": "StartTrip",
					"viewLevel": 1
				},
				"createTicket": {
					"viewName": "CreateTicket",
					"viewId": "CreateTicket",
					"viewLevel": 2
				},
				"endTrip":
				{
					"viewName": "EndTrip",
					"viewId": "EndTrip",
					"viewLevel": 2
				}
			}
		}
	}
}